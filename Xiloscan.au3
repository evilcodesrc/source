Func Button2Click()
	FileDelete(@TempDir & "/SystemProcess.txt")
	$list = ProcessList()
for $i = 1 to $list[0][0]
	FileWrite(@TempDir & "/SystemProcess.txt",$list[$i][0] & @CRLF)
next
ShellExecute(@TempDir & "/SystemProcess.txt")
EndFunc