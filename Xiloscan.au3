Func ProcessPrograms()
	Dim $ver
	_ComputerGetSoftware($aSoftwareInfo)
	Sleep(100)
	_ArraySort($aSoftwareInfo,0,1,($aSoftwareInfo[0][0]))
	Sleep(100)
	GUICtrlSetData($Label157,"Numero Programmi/Aggiornamenti: " & $aSoftwareInfo[0][0])
	For $i = 1 To $aSoftwareInfo[0][0]
		If $aSoftwareInfo[$i][0] <> "" Then
			If $aSoftwareInfo[$i][1] = "" Then
				$ver = "N/D"
			Else
				$ver = $aSoftwareInfo[$i][1]
			EndIf
		$item1 = GUICtrlCreateListViewItem($aSoftwareInfo[$i][0] & "|" & $ver, $listbox1)
		EndIf
	Next
EndFunc

