	_ComputerGetOSs($aOSInfo)
	$osinfo = StringSplit($aOSInfo[1][0],"|")
	GUICtrlSetData($Label60,$osinfo[1])
	GUICtrlSetData($Label60b,$osinfo[1])
	GUICtrlSetData($Label62,$aOSinfo[1][1])
	GUICtrlSetData($Label64,$aOSinfo[1][2])
	GUICtrlSetData($Label66,$aOSinfo[1][3])
	GUICtrlSetData($Label68,$aOSinfo[1][5])
	GUICtrlSetData($Label70,$aOSinfo[1][6])
	GUICtrlSetData($Label72,$aOSinfo[1][18])
	GUICtrlSetData($Label74,$aOSinfo[1][20] & " byte")
	GUICtrlSetData($Label76,$aOSinfo[1][21] & " byte")
	GUICtrlSetData($Label78,$aOSinfo[1][22] & " byte")
	GUICtrlSetData($Label80,$aOSinfo[1][23] & " (mese/giorno/anno)")
	GUICtrlSetData($Label82,$aOSinfo[1][26])
	GUICtrlSetData($Label84,$aOSinfo[1][27])
	GUICtrlSetData($Label86,$aOSinfo[1][28])
	GUICtrlSetData($Label88,$aOSinfo[1][32])
	GUICtrlSetData($Label90,$aOSinfo[1][45])
	GUICtrlSetData($Label92,$aOSinfo[1][46])
	GUICtrlSetData($Label94,$aOSinfo[1][49] & " byte")
	GUICtrlSetData($Label96,$aOSinfo[1][50])
	GUICtrlSetData($Label98,$aOSinfo[1][56] & " byte")
	GUICtrlSetData($Label100,$aOSinfo[1][57] & " byte")
	GUICtrlSetData($Label100b,Int($aOSinfo[1][57]/1000000) & " Gb (" & $aOSinfo[1][57] & " byte)")
	GUICtrlSetData($Label102,$aOSinfo[1][58])
	GUICtrlSetData($Label104,$aOSinfo[1][59])
EndFunc

